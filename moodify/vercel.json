{
    "version": 2,
    "buildCommand": "next build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "env": {
      "SPOTIFY_CLIENT_ID": "@spotify_client_id",
      "SPOTIFY_CLIENT_SECRET": "@spotify_client_secret",
      "SPOTIFY_REDIRECT_URI": "@spotify_redirect_uri",
      "OPENAI_API_KEY":"@openai_api_key"
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }